<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/getById/:saleAgentId" method="GET">
        <service class="AHT\Pike\Api\SaleAgentRepositoryInterface" method="getById"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>

    <route url="/V1/getEntityId" method="GET">
        <service class="AHT\Pike\Api\Data\SaleAgentInterface" method="getEntityId"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>

    <route url="/V1/getEntityId" method="GET">
        <service class="AHT\Pike\Api\Data\SaleAgentInterface" method="getEntityId"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>